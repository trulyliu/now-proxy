{
	root /app/wwwroot
	index index.html
	timeouts none
	proxy /v2vless 127.0.0.1:9080 {
		websocket
		header_upstream -Origin
	}
    proxy /v2vmess 127.0.0.1:9081 {
        websocket
        header_upstream -Origin
    }
    proxy /v2ray 127.0.0.1:9082 {
        websocket
        header_upstream -Origin
    }
}